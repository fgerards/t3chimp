<T3DataStructure>
    <meta>
		<langDisable>1</langDisable>
	</meta>
    <sheets>
        <main>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.subscriptionList>
                        <TCEforms>
                            <label>List</label>
                            <config>
                                <type>select</type>
                                <items></items>
                                <itemsProcFunc>MatoIlic\T3Chimp\Provider\FlexFormValues->getLists</itemsProcFunc>
                                <maxitems>1</maxitems>
								<size>1</size>
                            </config>
                        </TCEforms>
                    </settings.subscriptionList>
                    <settings.doubleOptIn>
                        <TCEforms>
                            <label>Double opt-in</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.doubleOptIn>
                    <settings.disableWelcomeEmail>
                        <TCEforms>
                            <label>Disable welcome email</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.disableWelcomeEmail>
                    <settings.showEmailFormat>
                        <TCEforms>
                            <label>Let subscribers pick email format</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.showEmailFormat>
                </el>
            </ROOT>
        </main>
    </sheets>
</T3DataStructure>

<T3DataStructure>
    <meta>
		<langDisable>1</langDisable>
	</meta>
    <sheets>
        <main>
            <ROOT>
                <TCEforms>
                    <sheetTitle>List</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.subscriptionList>
                        <TCEforms>
                            <label>LLL:EXT:t3chimp/Resources/Language/locallang.xml:flexForm.list</label>
                            <config>
                                <type>select</type>
                                <items></items>
                                <itemsProcFunc>Tx_T3chimp_Domain_Repository_ListRepository->getFlexFormValues</itemsProcFunc>
                                <maxitems>1</maxitems>
								<size>1</size>
                            </config>
                        </TCEforms>
                    </settings.subscriptionList>
                </el>
            </ROOT>
        </main>
        <templates>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Templates</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.fieldTemplate>
                        <TCEforms>
                            <label>LLL:EXT:t3chimp/Resources/Language/locallang.xml:flexForm.fieldTemplate</label>
                            <config>
                                <type>text</type>
								<cols>50</cols>
                                <rows>30</rows>
                                <default>
                                    &lt;p&gt;
	                                    %label%&lt;br /&gt;
	                                    %field%
                                    &lt;/p&gt;
                                </default>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.fieldTemplate>
                </el>
            </ROOT>
        </templates>
    </sheets>
</T3DataStructure>